<h1>Админ панель</h1>
<h5 style="color: red;">{{addTable}}</h5>
<form method="post" action="/adminpanel/addTables">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="number">Номер стола: </label>
            <input type="number" class="form-control" id="number" name="number" placeholder="Введите номер стола">

        </div>
        <div class="form-group col-md-6">
            <label for="places">Количество мест</label>
            <input type="number" class="form-control" id="places" name="places" >
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Добавить</button>
</form>


